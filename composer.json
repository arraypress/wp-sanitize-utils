{
  "name": "arraypress/wp-sanitize-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for sanitization and validation utilities",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-sanitize-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\SanitizeUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-sanitize-utils/issues",
    "source": "https://github.com/arraypress/wp-sanitize-utils"
  },
  "keywords": [
    "wordpress",
    "sanitization",
    "validation",
    "clean",
    "security",
    "data",
    "input",
    "filtering",
    "utilities",
    "arraypress"
  ]
}